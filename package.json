{
  "name": "dev-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "gen:openapi": "node scripts/generate-openapi.js",
    "predev": "node scripts/generate-openapi.js && npx prisma generate",
    "prebuild": "node scripts/generate-openapi.js && npx prisma generate",
    "dev": "next dev",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "docker:build:server": "rm -rf .next && npm run build && docker buildx build --platform linux/amd64 -t dev-portal:latest .",
    "docker:save": "docker save -o dev-portal.tar dev-portal:latest",
    "docker:pack": "npm run docker:build:server && npm run docker:save",
    "docker:dev": "docker compose up -d --build",
    "docker:stop": "docker compose down",
    "extension:pack": "node scripts/pack-extension.mjs"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/nextjs-registry": "^1.3.0",
    "@prisma/client": "^5.22.0",
    "antd": "^6.1.4",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "kbar": "^0.1.0-beta.48",
    "next": "16.1.1",
    "next-swagger-doc": "^0.4.1",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "swagger-ui-react": "^5.31.0",
    "tailwind-merge": "^3.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "prisma": "^5.22.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
